ServerAliveInterval 120
TCPKeepAlive no

Host *.addsrv.net *.addsrv.com *.rtapi.net *.virt
    ForwardAgent yes
    UserKnownHostsFile=/dev/null
    StrictHostKeyChecking=no
    LogLevel=error

Host *.compute.amazonaws.com
    RemoteForward 2022 gitlab.addsrv.com:22
    ForwardAgent yes
    IdentityFile ~/.ssh/ddemeshchuk.pem

Host p*
    ForwardAgent yes
    UserKnownHostsFile=/dev/null
    StrictHostKeyChecking=no
